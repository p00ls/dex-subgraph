{
  "name": "p00ls-dex-subgraph",
  "version": "1.0.0",
  "repository": "https://github.com/p00ls/dex-subgraph",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "build-staging": "graph build ${SUBGRAPH:-subgraph-staging.yaml}",
    "build-prod": "graph build ${SUBGRAPH:-subgraph-prod.yaml}",
    "codegen-staging": "graph codegen ${SUBGRAPH:-subgraph-staging.yaml} --output-dir src/types/",
    "codegen-prod": "graph codegen ${SUBGRAPH:-subgraph-prod.yaml} --output-dir src/types/",
    "deploy-staging": "graph deploy p00ls/dex-staging-goerli ./subgraph-staging.yaml --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/ --debug",
    "deploy-prod": "graph deploy p00ls/dex-production ./subgraph-prod.yaml --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/ --debug"
  },
  "devDependencies": {
    "@j'/graph-cli": "^0.16.0",
    "@graphprotocol/graph-ts": "^0.16.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "prettier": "^1.18.2",
    "typescript": "^3.5.2"
  }
}
